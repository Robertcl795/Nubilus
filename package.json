{
  "name": "nubilus",
  "version": "1.0.0",
  "description": "GraphQl API",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node --inspect=5858 src/server.ts",
    "dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "keywords": [
    "API",
    "GraphQl",
    "MongoDB"
  ],
  "author": "Robert Carrillo",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.2.1",
    "mongoose": "^5.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/jest": "^24.0.11",
    "@types/mongoose": "^5.3.26",
    "@types/node": "^11.13.4",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.3"
  }
}
